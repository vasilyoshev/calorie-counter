    "express": "4.16.3",
    "express-session": "^1.15.6",
    "express-validation": "^1.0.2",
    "hammerjs": "^2.0.8",
    "helmet": "^3.12.1",
    "joi": "^13.6.0",
    "mongoose": "5.1.1",
    "ngx-spinner": "^6.1.2",
    "nodemailer": "^4.6.8",
    "rxjs": "^6.2.0",
    "session-file-store": "^1.2.0",
    "supertest": "^3.1.0",
    "zone.js": "^0.8.19"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.6.5",
    "@angular/compiler-cli": "^6.1.3",
    "@angular/language-service": "^6.1.3",
    "@types/jest": "^23.3.1",
    "@types/node": "^6.0.116",
