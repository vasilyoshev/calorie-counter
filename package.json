    "babel-preset-latest": "^6.24.1",
    "babel-register": "^6.26.0",
    "codelyzer": "^4.4.4",
    "jest": "^23.5.0",
    "jest-preset-angular": "^6.0.0",
    "protractor": "~5.1.2",
    "ts-node": "~4.1.0",
    "tslint": "~5.9.1",
    "typescript": "~2.7.2",
    "rimraf": "~2.6.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/draawn/calorie-counter.git"
  },
  "author": "Vasil Yoshev",
